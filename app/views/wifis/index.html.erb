<%= render partial: "shared/admin_menu", locals: { title: "Wifi" } %>

<div class=" port-index-body ">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Network</th>
        <th>Password</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @wifis.each do |wifi| %>
        <tr>
          <td><%= wifi.name %></td>
          <td><%= wifi.network %></td>
          <td><%= wifi.password %></td>
          <td><%= link_to 'Edit', edit_wifi_path(wifi) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class=" link-new ">
    <%= link_to 'New Wifi', new_wifi_path %>
  </div>
</div>
